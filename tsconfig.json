{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "lib": ["dom", "es2015", "es2016", "es2017.object"],
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    "sourceMap": true,
    "outDir": "lib",
    "strict": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "baseUrl": "./src",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"]
}
